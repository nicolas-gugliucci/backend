<div>
    <h1>Lista de productos agregados hasta el momento del ingreso a la web:</h1>
    <h2>Recargar p√°gina para actualizar</h2>
    {{#each products}}
        <div>
            <p>Id: {{_id}}</p>
            <p>Title: {{title}}</p>
            <p>Description: {{description}}</p>
            <p>Code: {{code}}</p>
            <p>Price: {{price}}</p>
            <p>Status: {{status}}</p>
            <p>Stock: {{stock}}</p>
            <p>Category: {{category}}</p>
            {{#if thumbnails}}
                <p>Tumbnails:</p>
                {{#each thumbnails}}
                    <img style="height: 150px;" src="{{this}}"/>
                {{/each}}
            {{/if}}
        </div>
        <hr>
    {{/each}}
</div>
<div>
    {{#if prevLink}}
        <a href="{{prevLink}}">Anterior</a>
    {{/if}}
    {{#if nextLink}}
        <a href="{{nextLink}}">Siguiente</a>
    {{/if}}
</div>