<div>
    <h1>Lista de productos agregados hasta el momento del ingreso a la web:</h1>
    <h2>Recargar p√°gina para actualizar</h2>
    {{#each products}}
        <div>
            <p>Id: {{this._id}}</p>
            <p>Title: {{this.title}}</p>
            <p>Description: {{this.description}}</p>
            <p>Code: {{this.code}}</p>
            <p>Price: {{this.price}}</p>
            <p>Status: {{this.status}}</p>
            <p>Stock: {{this.stock}}</p>
            <p>Category: {{this.category}}</p>
            {{#if this.thumbnails}}
                <p>Tumbnails:</p>
                {{#each this.thumbnails}}
                    <p>{{this}}</p>
                {{/each}}
            {{/if}}
        </div>
        <hr>
    {{/each}}
</div>